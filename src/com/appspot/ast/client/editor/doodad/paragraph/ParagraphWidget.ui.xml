<ui:UiBinder
    xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:gwt='urn:import:com.google.gwt.user.client.ui'
    >
  <ui:style>
    .item {
      display: block;
      position: relative;
      margin: 1em;
      line-height: 1.5em;
    }

    .item > * {
      border: 1px solid white;
      background: transparent;
    }
  </ui:style>
  <gwt:HTMLPanel styleName='{style.item}'>
  </gwt:HTMLPanel>
</ui:UiBinder>
